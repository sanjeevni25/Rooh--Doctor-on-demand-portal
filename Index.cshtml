<script>
    //funtion to open menu
    function openSideMenu() {
        document.getElementById("side-nav").style.display = "block";
        document.getElementById("side-nav").style.width = "70%";
    }
    //funtion to close menu
    function closeSideMenu() {
        document.getElementById("side-nav").style.width = "0";
        document.getElementById("side-nav").style.display = "none";
    }

    function openSignUpMenu() {
        document.getElementById("modal_signup_account").style.display = "block";
        document.getElementById("modaluserlogin").style.display = "none";
        }
    


    function openLoginMenu() {
        document.getElementById("modaluserlogin").style.display = "block";
        document.getElementById("modal_signup_account").style.display = "none";
    }

</script>

<style>
    .menu.active {
        opacity: 1 !important;
        transform: translateY(0) !important;
        visibility: visible !important;
        position: absolute;
        z-index: 999999;
    }

    .showcase {
        width: 100%;
        height: 500px;
        position: relative;
        color: white;
        text-align: center;
    }

